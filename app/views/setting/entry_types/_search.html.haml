.search_box
  = search_form_for @search, :url => setting_entry_types_path, :html => {:method => :get} do |f|
    %table
      %td= f.label :name_cont
      %td= f.text_field :name_cont

    .search_buttons
      = button_tag 'Search', :class => 'button btn-search', :disable_with => 'Searching...', :tabindex => 4
      = link_to 'Clear', setting_entry_types_path, :class => 'button btn-reset', :tabindex => 5
      .page_entries= page_entries_info @entry_types
